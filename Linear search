#include<stdio.h>
#include<conio.h>
int main()
{
int arr[5],pos,i,num;
printf("enter numbers:");
for(i=0;i<5;i++)
scanf("%d",&arr[i]);
printf("enter number want to search:");
scanf("%d",&num);
for(i=0;i<5;i++)
{
if(arr[i]==num)
{
pos=i;
break;
}

}
printf("/nnumber found at .%d",pos);
getch();
return 0;

}
